let tree = {
  sum: 0
}

const files = Object.keys(data).filter(->(key) !["SUM", "header"].contains(key))

files.each(->(path) {
  const filedata = data[path]

  path = new Path(path)
  path.remove_first()

  const filename = path.filename()

  path.remove_last()

  let current = tree

  path.elements.each(->(folder) {
    unless current[folder] {
      current[folder] = {
        sum: 0
      }
    }

    current.sum += filedata.code
    current = current[folder]
  })

  current[filename] = filedata.code
  current.sum += filedata.code
})

func print_tree(tree, depth = 0) {
  const files = Object.keys(tree).filter(->$0 ! "sum")

  files.each(->(file) {
    if typeof tree[file] == "object" {
      print(file.indent(depth * 2) + " " + tree[file].sum)
      print_tree(tree[file], depth + 1)
    } else {
      print(file.indent(depth * 2) + " " + tree[file])
    }
  })
}

const filter = [
  "benchmarks",
  "bin",
  "build",
  "docs",
  "examples",
  "experiments",
  "feature-ideas",
  "libraries",
  "libs",
  "scratchpad",
  "vim_syntax_files",
  "test"
]
filter.each(->(f) Object.delete(tree, f))

tree = tree.filter(->(k, v) typeof v == "object")

print_tree(tree)
